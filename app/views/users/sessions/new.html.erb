<div class="row top-10">
    <div class="col-md-4 col-md-offset-4">
        <div class="login-panel panel panel-default">


<h2 style="text-align:center;">Log in</h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name), html: { class: "form" }, action: "update") do |f| %>
<form >



  <div class="form-group">
      <%= f.label :email, class: "exampleInputEmail1" %>
      <%= f.email_field :email, autofocus: true, class: "form-control", placeholder: "Email"%>
  </div>

  <div class="form-group">
        <%= f.label :senha%>
        <%= f.password_field :password, autocomplete: "off", class: "form-control", placeholder: "senha"%>

  </div>

  <% if devise_mapping.rememberable? %>


          <%= f.check_box :remember_me %>
          <%= f.label :remember_me%>
  <% end %>

  <div class="actions">
    <%= f.submit "Log in", class: "btn btn-default"%>
  </div>


<% end %>

<%= render "users/shared/links" %>
</div>

</div>
</div>
